<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="library.MainController">
   <children>
      <HBox style="-fx-background-color: white;">
         <children>
            <JFXButton fx:id="adminBtn" onAction="#loadAdministrator" prefHeight="250.0" prefWidth="250.0" stylesheets="@../main.css" text=" " textAlignment="JUSTIFY" textFill="WHITE">
               <graphic>
                  <ImageView fx:id="admin" fitHeight="200.0" fitWidth="230.0" pickOnBounds="true" preserveRatio="true" />
               </graphic>
            </JFXButton>
            <JFXButton fx:id="studentBtn" layoutX="10.0" layoutY="10.0" onAction="#loadStudent" prefHeight="250.0" prefWidth="250.0" stylesheets="@../main.css" text=" " textFill="WHITE">
               <graphic>
                  <ImageView fx:id="student" fitHeight="200.0" fitWidth="230.0" pickOnBounds="true" preserveRatio="true" />
               </graphic>
            </JFXButton>
         </children>
      </HBox>
   </children>
</AnchorPane>
