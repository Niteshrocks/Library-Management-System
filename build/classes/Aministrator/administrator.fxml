<AnchorPane id="AnchorPane" prefHeight="338.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Aministrator.AdministratorController">
   <children>
      <ImageView fx:id="background" fitHeight="360.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../Pictures/library/adminBackground.jpg" />
         </image>
      </ImageView>
      <JFXButton fx:id="backBtn" layoutX="14.0" layoutY="14.0" onAction="#goBack" ripplerFill="#ffffff00" text="Back" textFill="#570404" />
      <HBox layoutX="47.0" layoutY="69.0" spacing="10.0">
         <children>
            <JFXButton fx:id="addBtn" onAction="#addData" prefHeight="92.0" prefWidth="116.0" ripplerFill="#ffffff00" style="-fx-border-color: #bdbdbd;" stylesheets="@../main.css" text="ADD" textOverrun="CENTER_ELLIPSIS">
               <font>
                  <Font name="DejaVuMathTeXGyre-Regular" size="18.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="deleteBtn" onAction="#DeleteData" prefHeight="92.0" prefWidth="116.0" ripplerFill="#ffffff00" style="-fx-border-color: #bdbdbd;" stylesheets="@../main.css" text="DELETE" textOverrun="CENTER_ELLIPSIS">
               <font>
                  <Font name="DejaVuMathTeXGyre-Regular" size="18.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="viewBtn" layoutX="126.0" layoutY="10.0" onAction="#ViewData" prefHeight="92.0" prefWidth="116.0" ripplerFill="#ffffff00" style="-fx-border-color: #bdbdbd;" stylesheets="@../main.css" text="VIEW&#10;DATA" textOverrun="CENTER_ELLIPSIS">
               <font>
                  <Font name="DejaVuMathTeXGyre-Regular" size="18.0" />
               </font>
            </JFXButton>
         </children>
      </HBox>
   </children>
</AnchorPane>
